<%= display_errors %>
<h1>Register</h1>
<form id="form_id" action="/register" method="POST">
  <h2>Username</h2>
  <input type="text" name="user[permanent_name]">
  <h2>Password</h2>
  <input type="password" name="user[password]">
  <h2>Password Confirmation</h2>
  <input type="password" name="user[password_confirmation]">
  <input class="nav-green" type="submit" value="Register Account">
</form>
